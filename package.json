{
  "name": "sekhon-dog-kennel-website",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "CLOUDFLARE_ENV=dev vite",
    "build:dev": "CLOUDFLARE_ENV=dev vite build && tsc --noEmit",
    "build:prod": "vite build && tsc --noEmit",
    "typegen": "wrangler types",
    "preview": "vite preview",
    "format": "biome format",
    "lint": "biome lint",
    "fix": "biome check --write",
    "deploy:dev": "bun run build:dev && wrangler deploy --env=dev",
    "deploy:prod": "bun run build:prod && wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate:dev": "wrangler d1 migrations apply sekhon-dog-kennel-dev-db --env dev --remote",
    "db:migrate:local": "wrangler d1 migrations apply sekhon-dog-kennel-dev-db --env dev --local",
    "db:migrate:prod": "wrangler d1 migrations apply sekhon-dog-kennel-db --remote",
    "db:migrate:remote": "bun run db:migrate:dev && bun run db:migrate:prod",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@orpc/client": "^1.13.0",
    "@orpc/json-schema": "^1.13.0",
    "@orpc/openapi": "^1.13.0",
    "@orpc/server": "^1.13.0",
    "@orpc/tanstack-query": "^1.13.0",
    "@orpc/zod": "^1.13.0",
    "@std/encoding": "npm:@jsr/std__encoding",
    "@std/ulid": "npm:@jsr/std__ulid",
    "@tanstack/react-form": "^1.27.6",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.143.4",
    "@tanstack/react-router-ssr-query": "^1.143.4",
    "@tanstack/react-start": "^1.143.4",
    "@typescript/native-preview": "^7.0.0-dev.20251223.1",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/vite-plugin": "^1.19.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router-devtools": "^1.143.4",
    "@tanstack/router-plugin": "^1.143.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "wrangler": "^4.54.0"
  }
}
